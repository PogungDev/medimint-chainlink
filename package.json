{
  "name": "medimint",
  "version": "1.0.0",
  "description": "MediMint - Blockchain-based Medical Education Funding Platform",
  "main": "index.js",
  "scripts": {
    "dev": "next dev -p 8947",
    "dev:custom": "next dev -p 8947",
    "build": "next build",
    "start": "next start -p 8947",
    "lint": "next lint",
    "compile": "hardhat compile",
    "test": "hardhat test",
    "node": "hardhat node",
    "deploy": "hardhat run scripts/deploy-simple-arbitrum.js --network arbitrumSepolia",
    "deploy:arbitrum": "hardhat run scripts/deploy-simple-arbitrum.js --network arbitrumSepolia",
    "deploy:sepolia": "hardhat run scripts/deploy-ethereum-sepolia.js --network ethereumSepolia",
    "deploy:testnet": "hardhat run scripts/deploy-simple-arbitrum.js --network arbitrumSepolia",
    "deploy:all": "npm run compile && npm run deploy",
    "check-balance": "hardhat run scripts/check-balance.js --network arbitrumSepolia",
    "check-address": "hardhat run scripts/check-address-balance.js --network arbitrumSepolia",
    "update-key": "node scripts/update-private-key.js",
    "success": "cat DEPLOYMENT-SUCCESS.md",
    "functions:simulate": "hardhat functions-simulate",
    "verify": "hardhat verify --network arbitrumSepolia",
    "setup:arbitrum": "echo 'Setting up Arbitrum Sepolia...' && npm run compile && npm run deploy",
    "demo:flow": "node scripts/demo-flow.js",
    "install-deps": "npm install"
  },
  "keywords": [
    "blockchain",
    "defi",
    "education",
    "chainlink",
    "nft",
    "soulbound",
    "funding",
    "medical"
  ],
  "author": "MediMint Team",
  "license": "MIT",
  "dependencies": {
    "@chainlink/contracts": "^0.8.0",
    "@chainlink/contracts-ccip": "^0.7.6",
    "@openzeppelin/contracts": "^4.9.3",
    "@openzeppelin/contracts-upgradeable": "^4.9.3",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@rainbow-me/rainbowkit": "^1.3.0",
    "@supabase/supabase-js": "^2.38.0",
    "@tanstack/react-query": "^4.36.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "dotenv": "^17.0.0",
    "ethers": "^6.8.0",
    "lucide-react": "^0.294.0",
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redis": "^4.6.10",
    "tailwind-merge": "^2.0.0",
    "viem": "^1.19.0",
    "wagmi": "^1.4.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@nomicfoundation/hardhat-verify": "^1.1.0",
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.52.0",
    "eslint-config-next": "^14.0.0",
    "hardhat": "^2.17.0",
    "hardhat-gas-reporter": "^1.0.9",
    "postcss": "^8.4.0",
    "solidity-coverage": "^0.8.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.2.0"
  },
  "type": "commonjs",
  "optionalDependencies": {
    "pino-pretty": "^13.0.0"
  }
}
